  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceRoot}/event-handler/sample/index.js",
      "sourceMaps": true,
      "outFiles": [
        "examples/**/*.ts",
        "src/**/*.ts"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Migrate",
      "program": "${workspaceRoot}/ingestor/migrate-db/index.js",
      "sourceMaps": true,
      "outFiles": [
        "examples/**/*.ts",
        "command-handler/**/*.t",
        "ingestor/**/*.ts",
        "src/**/*.ts"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Port",
      "address": "localhost",
      "port": 5858,
      "outFiles": []
    }
  ]
}